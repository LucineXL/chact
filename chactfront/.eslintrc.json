{
    "ecmaFeatures": {
        "modules": true
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "import/no-extraneous-dependencies": 0,
        "class-methods-use-this": 0,
        "indent": "off",
        "experimentalDecorators":0
    }
}